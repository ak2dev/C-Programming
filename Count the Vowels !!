#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char s[1001];
    fgets(s, sizeof(s), stdin);
    
    size_t len = strlen(s);
    if (len > 0 && s[len - 1] == '\n') {
        s[len - 1] = '\0';
        len--;
    }
    
    int count = 0;
    for (size_t i = 0; i < len; i++) {
        char lower = tolower(s[i]);
        if (lower == 'a' || lower == 'e' || lower == 'i' || lower == 'o' || lower == 'u') {
            count++;
        }
    }
    
    printf("%d\n", count);
    return 0;
}
