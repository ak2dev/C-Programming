#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int arr[N];

    // Read N integers into the array
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    // Print elements in reverse order
    for (int i = N - 1; i >= 0; i--) {
        printf("%d", arr[i]);
        if (i != 0) {
            printf(" ");  // Print space after each element except the last
        }
    }
    printf("\n");

    return 0;
}
