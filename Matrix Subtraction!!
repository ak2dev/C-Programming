#include <stdio.h>

int main() {
    int R, C;

    // Read number of rows and columns
    if (scanf("%d %d", &R, &C) != 2) {
        printf("Invalid input for matrix dimensions.\n");
        return 1;
    }

    // Validate constraints
    if (R < 1 || R > 100 || C < 1 || C > 100) {
        printf("Matrix dimensions out of allowed range (1 to 100).\n");
        return 1;
    }

    int matrix1[100][100];
    int matrix2[100][100];
    int result[100][100];

    // Input first matrix
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            if (scanf("%d", &matrix1[i][j]) != 1) {
                printf("Invalid input for first matrix element.\n");
                return 1;
            }
        }
    }

    // Input second matrix
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            if (scanf("%d", &matrix2[i][j]) != 1) {
                printf("Invalid input for second matrix element.\n");
                return 1;
            }
        }
    }

    // Perform matrix subtraction
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            result[i][j] = matrix1[i][j] - matrix2[i][j];
        }
    }

    // Output result matrix
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            printf("%d", result[i][j]);
            if (j < C - 1) printf(" ");
        }
        printf("\n");
    }

    return 0;
}
