#include <stdio.h>
#include <ctype.h>  // for isalpha() and tolower()

int main() {
    char ch;
    
    // Read a single character input
    scanf("%c", &ch);
    
    // Check if input is an alphabet
    if (!isalpha(ch)) {
        printf("Invalid Input\n");
        return 0;
    }
    
    // Convert character to lowercase for uniform checking
    ch = tolower(ch);
    
    // Check for vowels
    if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
        printf("Vowel\n");
    } else {
        printf("Consonant\n");
    }
    
    return 0;
}
