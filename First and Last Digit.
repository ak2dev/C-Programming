#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int num, first, last= 0;
        scanf("%d",&num);
        last = num%10;
        while (num!=0)
        {
            first = num%10;
            num=num/10;

        }
        printf("%d %d",first,last);
    return 0;
}
