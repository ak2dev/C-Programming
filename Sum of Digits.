#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main(){
        int num, rem, sum = 0;
        
        scanf("%d",&num);
        while (num!=0)
        {
            rem = num%10;
            // printf("%d ",rem);
            num=num/10;
            sum = sum + rem;
        }
        printf("%d",sum);
        return 0;
}
