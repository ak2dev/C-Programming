#include <stdio.h>

int main() {
    int totalDays, years, weeks, days;
    
    // Input total number of days
    scanf("%d", &totalDays);
    
    // Calculate years (1 year = 365 days)
    years = totalDays / 365;
    
    // Calculate remaining days after years
    totalDays = totalDays % 365;
    
    // Calculate weeks from remaining days (1 week = 7 days)
    weeks = totalDays / 7;
    
    // Calculate remaining days after weeks
    days = totalDays % 7;
    
    // Output the result
    printf("Years: %d\n", years);
    printf("Weeks: %d\n", weeks);
    printf("Days: %d\n", days);
    
    return 0;
}
