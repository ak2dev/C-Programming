#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int reversed = 0;
    int sign = (N < 0) ? -1 : 1;
    N = N * sign;  // Work with positive value for simplicity

    while (N > 0) {
        int digit = N % 10;
        reversed = reversed * 10 + digit;
        N /= 10;
    }

    reversed *= sign;  // Restore original sign

    printf("%d\n", reversed);
    return 0;
}
