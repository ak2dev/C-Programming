#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int arr[N];
    int sum = 0;

    // Read N integers into the array
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];  // Add each element to sum
    }

    // Print the sum
    printf("%d\n", sum);

    return 0;
}
