#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int arr[N];
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    int key;
    scanf("%d", &key);

    int found = 0;  // Flag to indicate if key is found
    for (int i = 0; i < N; i++) {
        if (arr[i] == key) {
            printf("Element found at position %d\n", i + 1);
            found = 1;
            break;  // Stop searching once found
        }
    }

    if (!found) {
        printf("Element not found\n");
    }

    return 0;
}
